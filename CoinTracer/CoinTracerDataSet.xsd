                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select t.ID, t.SourceID, t.Zeitpunkt,
case 
    when t.TradeTypID = 5 and ps.Eigen = 1 then 69
    when (t.TradeTypID = 3 or t.TradeTypID = 4) and ks.IstFiat = 0 and kt.IstFiat = 0 then 19
    else t.TradeTypID
end as TradeTypID,
t.QuellPlattformID, t.QuellBetrag, t.QuellKontoID,
t.ZielPlattformID, t.ZielBetrag, t.ZielKontoID,
t.WertEUR, t.Info, t.BetragNachGebuehr,
t.ImportPlattformID, t.ImportID, t.RefTradeID, t.InTradeID, t.OutTradeID,
t.ZeitpunktZiel, t.Entwertet, t.Steuerirrelevant, 
t.QuellBetragNachGebuehr, t.Kommentar, t.InZeitpunkt
from Trades t
inner join Konten as ks on ks.ID = t.QuellKontoID
inner join Konten as kt on kt.ID = t.ZielKontoID
inner join Plattformen as pt on pt.ID = t.ZielPlattformID
inner join Plattformen as ps on ps.ID = t.QuellPlattformID
left join TradeTx tx on ((tx.InTradeID = t.ID or tx.OutTradeID = t.ID or tx.InTransferID = t.ID) 
	and tx.SzenarioID = @SzenarioID)
where t.Entwertet = 0 and t.Steuerirrelevant = 0 and t.Zeitpunkt &lt; @UntilDate and t.TradeTypID in (3, 4, 5, 7)
group by t.ID
having count(tx.TxID) = 0
order by t.Zeitpunkt, t.ID desc</CommandText>
